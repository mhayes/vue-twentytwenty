{
  "name": "vue-twentytwenty",
  "version": "1.0.0-1",
  "author": "Mark Hayes <me@markhay.es>",
  "scripts": {
    "serve": "vue-cli-service serve demo/index.js",
    "build": "vue-cli-service build --target lib --name vue-twentytwenty ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/vue-twentytwenty.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/compiler-sfc": "^3.2.24",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue": "^3.2.24"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "contributors": [
    "Mattia Astorino (https://equinsuocha.io)",
    "Michele Riva (https://micheleriva.it)",
    "Estee Tey (https://esteeytey.dev)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhayes/vue-twentytwenty"
  }
}
